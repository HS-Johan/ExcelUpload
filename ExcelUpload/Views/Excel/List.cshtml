@model List<ExcelUpload.ViewModel.ProductVM>

@{
    ViewData["Title"] = "Excel Data List";
}

<h2>Uploaded Excel Data</h2>

@if (Model != null && Model.Count > 0)
{
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>ProductName</th>
                <th>ProductDescription</th>
                <th>SKU</th>
                <th>ProductCategory</th>
                <th>SupplierId</th>
                <th>Stock</th>
                <th>IsPublished</th>
                <th>ActiveStartDate</th>
                <th>SellPrice</th>
                <th>BuyPrice</th>
                <th>EmployeeCost</th>
                <th>OtherCost</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.ProductName</td>
                    <td>@item.ProductDescription</td>
                    <td>@item.Sku</td>
                    <td>@item.ProductCategory</td>
                    <td>@item.SupplierId</td>
                    <td>@item.Stock</td>
                    <td>@item.IsPublished</td>
                    <td>@item.ActiveStartDate</td>
                    <td>@item.SellPrice</td>
                    <td>@item.BuyPrice</td>
                    <td>@item.EmployeeCost</td>
                    <td>@item.OtherCost</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No data found.</p>
}
