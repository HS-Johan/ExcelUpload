@model List<ExcelUpload.Models.ExcelData>

@{
    ViewData["Title"] = "Excel Data List";
}

<h2>Uploaded Excel Data</h2>

@if (Model != null && Model.Count > 0)
{
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Name</th>
                <th>Age</th>
                <th>City</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Column1</td>
                    <td>@item.Column2</td>
                    <td>@item.Column3</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No data found.</p>
}
